            <div style="margin-top: 20px;" class="table-responsive">
                <table id="example1" class="table table-striped table-sm">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>Event</th>
                        <th>Name</th>
                        <th>Description</th>
                        <th>Rate</th>
                    </tr>
                    </thead>
                    <tbody>
                    <c:forEach var="feedback" items="${listFeedback}" varStatus="status">

                        <tr>
                        <c:forEach var="contact" items="${listContact}" varStatus="status">
                            <c:if test="${contact.id_event == feedback.id_event}">
                            <form:form id="event_page" modelAttribute="eventPage" action="event_page" class="form-horizontal"   method="GET">
                                <input type="hidden" path="id_ev" name="id_ev" id="id_ev" placeholder="id_ev" value="${feedback.id_event}" class="form-control" >
                                <td>${status.index + 1}</td>
                                <td>${contact.nameEvent}</td>
                                <td>${feedback.name}</td>
                                <td>${feedback.description}</td>
                                <td>${feedback.rating}</td>
                            </form:form>

                            </c:if>
                        </c:forEach>
                        </tr>

                    </c:forEach>

                    </tbody>
                </table>
            </div>
        </main>
    </div>
</div>


