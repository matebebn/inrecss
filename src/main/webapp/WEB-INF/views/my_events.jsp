                <table id="example" class="table table-striped table-sm">
                    <thead>
                    <tr>
                        <th>#</th>
                        <th>Event</th>
                        <th>Rating</th>
                        <th>Person limit</th>
                        <th>Type event</th>
                        <th>Date</th>
                        <th>Start time</th>
                        <th>Address</th>
                        <th>Price</th>
                        <th>Event page</th>

                    </tr>
                    </thead>
                    <tbody>
                    <c:forEach var="contact" items="${listContact}" varStatus="status">
                        <c:set var="total" value="${0}"/>
                        <c:set var="countNumberRating" value="${0}"/>
                            <tr>
                        <c:forEach var="feedback" items="${listFeedback}" varStatus="xd">
                            <form:form id="event_page" modelAttribute="eventPage" action="event_page" class="form-horizontal"   method="GET">
                                    <c:if test="${contact.id_event == feedback.id_event}">
                                        <c:set var="total" value="${total + feedback.rating}"/>
                                        <c:set var="countNumberRating" value="${countNumberRating+1}"/>
                                    </c:if>

                                    <c:if test="${listFeedback.size()-1==xd.index}">

                                        <c:set var="total" value="${total/countNumberRating}" />
                                    <input type="hidden" path="id_ev" name="id_ev" id="id_ev" placeholder="id_ev" value="${contact.id_event}" class="form-control" >
                                    <td>${status.index + 1}</td>
                                    <td>${contact.nameEvent}</td>
                                        <td><c:if test="${total!='NaN'}"><h4>${total}</h4></c:if><c:if test="${total=='NaN'}">Not yet rated</c:if> </td>
                                    <td>${contact.personLimit}</td>
                                    <td><c:if test="${contact.eventType == '1'}">Paid</c:if><c:if test="${contact.eventType == '0'}">Free</c:if></td>
                                    <td>${contact.eventDate}</td>
                                    <td>${contact.startTimeEvent}</td>
                                    <td>${contact.adressEvent}</td>
                                    <td>${contact.priceEvent}</td>
                                    <td><button style="margin-top: 5%;margin-bottom:5%;"  type="submit"  class="btn btn-success btn-block">Enter</button></td>
                                    </c:if>
                            </form:form>
                        </c:forEach>
                            </tr>


                        </c:forEach>

                    </tbody>
                </table>
            </div>
        </main>


        <main class="col-md-9 ms-sm-auto col-lg-10 px-md-4">
            <div class="d-flex justify-content-between flex-wrap flex-md-nowrap align-items-center pt-3 pb-2 mb-3 border-bottom">
                <h2 class="h2">
                    <svg xmlns="http://www.w3.org/2000/svg" width="46" height="46" fill="currentColor" class="bi bi-calendar-check" viewBox="0 0 16 16">
